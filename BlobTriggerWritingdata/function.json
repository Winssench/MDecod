{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "myblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "iothub/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "path": "images/{name}/{DateTime}.jpg",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
